<launch>
    <arg name="namespace" default=""/>

    <node name="trajectory_executor_$(arg namespace)" pkg="eh_traj" type="trajectory_executor.py" output="screen" respawn="true">
        
        <param name="namespace" value="$(arg namespace)" />

        <rosparam>
            takeoff_height: 1.5
            max_velocity: 1.0
            goal_tolerance: 0.2
            control_rate: 20.0
        </rosparam>

    </node>
</launch>